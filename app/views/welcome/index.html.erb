
<div data-controller="welcome" data-welcome-index-value="1">
  <button data-action="welcome#previous"> ← </button>
  <button data-action="welcome#next"> → </button>

  <div data-welcome-target="slide">🐵</div>
  <div data-welcome-target="slide">🙈</div>
  <div data-welcome-target="slide">🙉</div>
  <div data-welcome-target="slide">🙊</div>
</div>

<div data-controller='welcome'>
  <%= render 'UI/navigation' %>

  <div class='p-4 xl:p-12 xl:mx-auto xl:container'>
    <%= render 'UI/banner' %>
    <h2 class='uppercase font-black text-4xl'>product Overview</h2>

    <%= render 'UI/products'%>

  </div>
  <%= render 'UI/footer'%>
</div>

